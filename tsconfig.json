{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2017",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "sourceMap": false,
    "outDir": "out",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strict": true
  },
  "exclude": [
    "node_modules"
  ]
}